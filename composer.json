{
    "name": "acdh-oeaw/arche-lib",
    "type": "library",
    "description": "A client library for the ACDH digital objects repository",
    "keywords": [],
    "homepage": "https://github.com/acdh-oeaw/arche-lib",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateusz Żółtak",
            "email": "mzoltak@oeaw.ac.at"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-yaml": "*",
        "guzzlehttp/guzzle": "^7",
        "zozlak/rdf-constants": "^1",
        "zozlak/query-part": "^2",
        "psr/log": "^2 | ^3",
        "sweetrdf/quick-rdf": "^2",
        "sweetrdf/quick-rdf-io": "^1.0.6"
    },
    "autoload": {
        "psr-4": {
            "acdhOeaw\\": "src/acdhOeaw"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "acdhOeaw\\arche\\lib\\tests\\": "tests"
        }
    }
}
